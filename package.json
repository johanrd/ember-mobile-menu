    "ember-cli-babel": "^7.21.0",
    "ember-cli-htmlbars": "^5.2.0",
    "ember-concurrency": "^1.1.6",
    "ember-concurrency-test-waiter": "^0.3.1",
    "ember-gesture-modifiers": "^0.2.0",
    "ember-maybe-import-regenerator": "^0.1.6",
    "ember-on-resize-modifier": "^0.3.0",
    "ember-set-body-class": "^0.4.0",
    "ember-usable": "https://github.com/pzuraq/ember-usable#0d03a500a2f49041a4ddff0bb05b077c3907ed7d",
    "tracked-maps-and-sets": "^2.0.0",
    "wobble": "^1.5.1"
  },
  "devDependencies": {
    "@ember/optional-features": "^1.3.0",
    "@embroider/test-setup": "^0.27.0",
    "babel-eslint": "^10.1.0",
    "broccoli-asset-rev": "^3.0.0",
    "ember-cli": "~3.20.0",
    "ember-cli-addon-docs": "^0.9.0",
    "ember-cli-addon-docs-yuidoc": "^0.2.4",
    "ember-cli-dependency-checker": "^3.2.0",
    "ember-cli-deploy": "^1.0.2",
    "ember-cli-deploy-build": "^1.1.1",
    "ember-cli-deploy-git": "^1.3.3",
    "ember-cli-deploy-git-ci": "^1.0.1",
    "ember-cli-inject-live-reload": "^2.0.2",
    "ember-cli-sass": "^10.0.0",
    "ember-cli-sri": "^2.1.1",
    "ember-cli-uglify": "^3.0.0",
    "ember-disable-prototype-extensions": "^1.1.3",
    "ember-export-application-global": "^2.0.1",
    "ember-load-initializers": "^2.1.1",
    "ember-qunit": "^4.6.0",
    "ember-resize-observer-polyfill": "^0.0.1",
    "ember-resolver": "^8.0.0",
    "ember-sinon": "^5.0.0",
    "ember-sinon-qunit": "^5.0.0",
    "ember-source": "~3.20.2",
    "ember-source-channel-url": "^3.0.0",
    "ember-template-lint": "^2.9.1",
    "ember-try": "^1.4.0",
    "eslint": "^7.5.0",
    "eslint-plugin-ember": "^8.9.1",
    "eslint-plugin-node": "^11.1.0",
    "loader.js": "^4.7.0",
    "npm-run-all": "^4.1.5",
    "qunit-dom": "^1.2.0",
    "sass": "^1.17.2"
  },
  "engines": {
    "node": "10.* || >= 12"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
